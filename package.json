{
	"name": "sqnc",
	"version": "2.0.13",
	"description": "Numeric and symbolic sequence generator, with utf16 support",
	"main": "dist/js/sqnc.js",
	"module": "index.js",
	"engines": {
		"node": ">=6.0.0"
	},
	"_moduleAliases": {
		"sqnc": "./src/js/sqnc.js"
	},
	"scripts": {
		"test": "eslint . && NODE_ENV=test BABEL_ENV=test webpack --config ./webpack/test.config.js && nyc --reporter=lcov node ./test/build/index.js | faucet && nyc report",
		"coverage": "codecov",
		"documentation": "documentation build src/js/index.js -f md > DOCUMENTATION.md",
		"fix": "eslint --fix ./src/js/",
		"clean-js": "rm -rf ./dist/js/ > /dev/null",
		"build": "run-s clean-js bump build:*",
		"bump": "npm version patch --no-git-tag-version",
		"build:js": "NODE_ENV=production BABEL_ENV=production webpack --config ./webpack/production.config.js",
		"release-commit": "node ./build-utils/run-esm.js -s ./build-utils/commit.js",
		"release": "npm run plain-release",
		"analyse-build": "NODE_ENV=production BABEL_ENV=production webpack --config ./webpack/production.config.js --profile",
		"analyse-build-2": "webpack-bundle-analyzer ./webpack/analyse.json ./dist/js/",
		"plain-release": "npm run build && va-release -g",
		"patch-release": "npm run build && va-release --version patch -g",
		"minor-release": "npm run build && va-release --version minor -g",
		"major-release": "npm run build && va-release --version major -g"
	},
	"va-release": {
		"assets": "dist/**/*",
		"files": [
			{
				"cwd": "templates/",
				"src": [
					"**/*"
				],
				"dest": "./"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forceuser/sqnc.git"
	},
	"keywords": [
		"sequence",
		"generator",
		"range",
		"step"
	],
	"author": {
		"name": "Vitaly Dub",
		"url": "https://github.com/forceuser"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/forceuser/sqnc/issues"
	},
	"homepage": "https://github.com/forceuser/sqnc#readme",
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0",
		"@babel/plugin-proposal-unicode-property-regex": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/plugin-transform-regenerator": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/register": "^7.0.0",
		"autoprefixer": "^9.1.5",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.2",
		"babel-minify": "^0.4.3",
		"babel-plugin-istanbul": "^5.1.0",
		"bestzip": "^2.1.1",
		"browser-sync": "^2.24.7",
		"clean-css-cli": "^4.2.1",
		"copyfiles": "^2.1.0",
		"cross-env": "^4.0.0",
		"deepmerge": "^2.2.1",
		"documentation": "^9.0.0",
		"eslint": "^5.10.0",
		"esm": "^3.0.84",
		"expose-loader": "^0.7.5",
		"express": "^4.16.3",
		"faucet": "0.0.1",
		"find-free-port": "^2.0.0",
		"foreach-cli": "^1.8.0",
		"fs-extra": "^7.0.0",
		"globby": "^8.0.1",
		"html-escape": "^2.0.0",
		"html-loader": "^0.4.5",
		"http-proxy-middleware": "^0.19.0",
		"imports-loader": "^0.7.1",
		"inject-loader": "^3.0.0",
		"is-glob": "^4.0.0",
		"is-wsl": "^1.1.0",
		"less": "^3.8.1",
		"module-alias": "^2.1.0",
		"morgan": "^1.9.1",
		"mustache": "^3.0.0",
		"ngrok": "^3.1.0",
		"npm-run-all": "^4.1.3",
		"nyc": "^13.1.0",
		"open": "0.0.5",
		"postcss-cli": "^6.0.0",
		"raw-loader": "^0.5.1",
		"regenerator-runtime": "^0.12.1",
		"semver": "^5.5.1",
		"serve-index": "^1.9.1",
		"serve-static": "^1.13.2",
		"shelljs": "^0.7.8",
		"sinon": "^2.2.0",
		"tape": "^4.6.3",
		"uglifycss": "0.0.29",
		"uglifyjs-webpack-plugin": "^2.0.1",
		"universal-url": "^1.0.1",
		"va-release": "^1.0.22",
		"watch-cli": "^0.2.3",
		"webpack": "4.20.2",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-cli": "3.1.1",
		"webpack-dev-middleware": "3.4.0",
		"webpack-dev-server": "3.1.8",
		"webpack-hot-middleware": "2.24.2",
		"webpack-merge": "4.1.4",
		"yargs": "^12.0.2"
	},
	"tonicExampleFilename": "tonic-example.js"
}
